<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Product>iSukces.Math</Product>
    <ProjectGuid>{F65D90E2-7284-40AD-8D61-A27A027B835E}</ProjectGuid>
    <TargetFrameworks>net6.0;net6.0-windows;net8.0;net8.0-windows;net9.0;net9.0-windows;net10.0;net10.0-windows</TargetFrameworks>
    <RootNamespace>iSukces.Mathematics</RootNamespace>
    <AssemblyVersion>1.25.1203.132</AssemblyVersion>
    <FileVersion>1.25.1203.132</FileVersion>
    <Version>1.25.1203.132</Version>
    <LangVersion>14</LangVersion>
    <Nullable>enable</Nullable>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(SignKey)</AssemblyOriginatorKeyFile>
    <NoWarn>1701;1702;CS1591;CS1572;CS1573;CS1591</NoWarn>
    <Description>Użyteczne klasy i metody do nieco bardziej zaawansowanej matematyki</Description>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    <PackageId>iSukces.Math</PackageId>
    <WarningLevel>3</WarningLevel>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <IncludeSource>false</IncludeSource>
    <IncludeSymbols>true</IncludeSymbols>
    <PackageProjectUrl>https://github.com/isukces/iSukces.Math</PackageProjectUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <RepositoryUrl>https://github.com/isukces/iSukces.Math</RepositoryUrl>
  </PropertyGroup>
  <ItemGroup>
    <Compile Update="Features\TypeConverters\GenericTypeConverter.cs">
      <DependentUpon>MathTypeConverter.cs</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="8.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>
</Project>