<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Company>Internet Sukces Piotr Stęclik</Company>
    <Authors>Internet Sukces Piotr Stęclik</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Copyright © Internet Sukces Piotr Stęclik 2017-2023</Copyright>
    <TargetFrameworks>net48;netstandard2.0;netcoreapp3.1;net6.0;net6.0-windows;net7.0;net7.0-windows</TargetFrameworks>
    <AssemblyOriginatorKeyFile>key.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Product>iSukces.Math</Product>
    <PackageId>iSukces.Math</PackageId>
    <Description>Użyteczne klasy i metody do nieco bardziej zaawansowanej matematyki</Description>
    <IncludeSource>false</IncludeSource>
    <IncludeSymbols>true</IncludeSymbols>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    <AssemblyVersion>1.0.23140.124</AssemblyVersion>
    <Version>1.0.23140.124</Version>
    <FileVersion>1.0.23140.124</FileVersion>
    <RootNamespace>iSukces.Mathematics</RootNamespace>
    <PackageProjectUrl>https://github.com/isukces/iSukces.Math</PackageProjectUrl>
    <NoWarn>1701;1702;CS1591;CS1572;CS1573;CS1591</NoWarn>
    <LangVersion>10</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="2022.3.1" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Features\TypeConverters\GenericTypeConverter.cs">
      <DependentUpon>MathTypeConverter.cs</DependentUpon>
    </Compile>
  </ItemGroup>
</Project>